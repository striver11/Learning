{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "S3ReadAccess",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:ListBucket"
			],
			"Resource": [
				"arn:aws:s3:::apdgen-data-bucket2",
				"arn:aws:s3:::apdgen-data-bucket2/*"
			]
		},
		{
			"Sid": "CloudWatchLogsAccess",
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogGroup",
				"logs:CreateLogStream",
				"logs:PutLogEvents"
			],
			"Resource": "*"
		}
	]
}
